                  <property name="styleSheet">
                   <string notr="true">/* 基础样式设置 */
QComboBox {
    border: 1px solid #d0d0d0;
    border-radius: 8px;
    padding: 7px 35px 7px 15px;
    min-width: 150px;
    font-family: &quot;Microsoft YaHei&quot;, sans-serif;
    font-size: 14px;
    color: #333333;
    background-color: #ffffff;
    selection-color: #ffffff;
    selection-background-color: #2a80eb;
}

/* 悬停状态 */
QComboBox:hover {
    border-color: #2a80eb;
    background-color: #f8fafc;
}

/* 禁用状态 */
QComboBox:disabled {
    border-color: #e0e0e0;
    color: #a0a0a0;
    background-color: #f5f5f5;
}

/* 下拉按钮区域 */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left: 1px solid #f0f0f0;
}

/* 下拉箭头 */
QComboBox::down-arrow {
    image: url(:/icons/arrow-down.png); /* 可替换为自己的箭头图标 */
    width: 16px;
    height: 16px;
    margin-right: 5px;
}

/* 下拉箭头在不同状态下的样式 */
QComboBox:hover::down-arrow {
    image: url(:/icons/arrow-down-hover.png);
}

QComboBox:on::down-arrow {
    image: url(:/icons/arrow-up.png); /* 下拉时显示向上箭头 */
}

/* 下拉列表样式 */
QComboBox QAbstractItemView {
    border: 1px solid #e0e0e0;
    border-radius: 8px;
    padding: 5px 0;
    margin: 4px 0 0 0;
    background-color: #ffffff;
    selection-background-color: #e6f0ff;
    selection-color: #2a80eb;
    font-family: &quot;Microsoft YaHei&quot;, sans-serif;
    font-size: 14px;
    outline: none;
    /* 阴影效果 */
    border: 1px solid rgba(0, 0, 0, 0.1);
    box-shadow: 0 6px 16px rgba(0, 0, 0, 0.08), 0 3px 6px rgba(0, 0, 0, 0.05);
}

/* 下拉列表项样式 */
QComboBox QAbstractItemView::item {
    padding: 10px 15px;
    margin: 0;
    color: #333333;
}

/* 下拉列表项悬停样式 */
QComboBox QAbstractItemView::item:hover {
    background-color: #f5f8ff;
    color: #2a80eb;
}

/* 下拉列表项选中样式 */
QComboBox QAbstractItemView::item:selected {
    background-color: #e6f0ff;
    color: #2a80eb;
}

/* 下拉列表滚动条样式 */
QComboBox QScrollBar:vertical {
    width: 8px;
    background: transparent;
    margin: 5px 0;
}

QComboBox QScrollBar::handle:vertical {
    background-color: #c0c0c0;
    border-radius: 4px;
    min-height: 30px;
}

QComboBox QScrollBar::handle:vertical:hover {
    background-color: #a0a0a0;
}

QComboBox QScrollBar::add-line:vertical,
QComboBox QScrollBar::sub-line:vertical {
    height: 0;
}</string>
